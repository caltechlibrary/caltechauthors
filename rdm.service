[Unit]
Description=Launch InvenioRDM Web Interface

[Service]
Type=simple
User=tmorrell
StandardOutput=kmsg
StandardError=kmsg
WorkingDirectory=/Sites/caltechauthors
EnvironmentFile=/Sites/caltechauthors/.env
ExecStart=/usr/local/bin/pipenv run uwsgi --logger syslog:inveniordm_ui uwsgi_ui.ini

[Install]
WantedBy=multi-user.target
